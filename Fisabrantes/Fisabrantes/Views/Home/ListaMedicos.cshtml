@model ICollection<Fisabrantes.Models.Funcionarios>

@{
    ViewBag.Title = "ListaMedicos";
}

<h2>Lista de Médicos</h2>


<div class="form-horizontal">

    @foreach (var medico in Model)
    {
        <div class="form-group">
            @Html.LabelFor(model => medico.Nome, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DisplayFor(model => medico.Nome, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => medico.DataEntClinica, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DisplayFor(model => medico.DataEntClinica, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>

    <hr />

    }

</div>






