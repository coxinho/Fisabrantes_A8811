
@{
    ViewBag.Title = "ListaUtentes";
    Layout = "~/Views/Home/About.cshtml";
}

<h2>ListaUtentes</h2>

<div class="form-horizontal">

    @foreach (var utente in Model)
    {
        <div class="form-group">
            @Html.LabelFor(model => utente.Nome, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DisplayFor(model => utente.Nome, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => ListaDeConsultas, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DisplayFor(model => ListaDeConsultas, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>

        <hr />

    }

</div>